
<% if(icon.search(/dtpr_icons/) > -1) { %>
  <img src="<%= icon %>" />
<% } else if (icon.search(/hex-icon/) > -1) { %>
  <div class="align-middle hex-icon">
    <img src="images/hexagon.svg" />
    <i class="material-icons md-24"><%= icon.split("hex-icon-")[1] %></i>
  </div>
<% } else if (icon.search(/circle-icon/) > -1) { %>
  <div class="align-middle circle-icon">
    <i class="material-icons md-18"><%= icon.split  ("circle-icon-")[1] %></i>
  </div>
<% } else if (icon.search(/number-icon/) > -1) { %>
  <div class="align-middle number-icon">
    <%= icon.split  ("number-icon-")[1] %>
  </div>
<% } else { %>
  <i class="material-icons md-36"><%= icon %></i>
<% } %>