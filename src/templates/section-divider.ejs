<div class="container sticky-nav">
  <div class="row">
    <div class="col-10">
      <h4 class="mb-0">
        <% if(index<0){ %>
          <%= require("ejs-loader!./icon.ejs")({icon:"circle-icon-lock"}) %>
        <% }else{ %>
          <%= require("ejs-loader!./icon.ejs")({icon:"number-icon-"+(index+1)}) %>
        <% } %>
        <span class="pl-1"><%= title %></span>
      </h4>
    </div>
    <div class="col-2 menu">
      <%= require("ejs-loader!./icon.ejs")({icon:"circle-icon-arrow_upward"}) %>
      <%= require("ejs-loader!./icon.ejs")({icon:"circle-icon-arrow_downward"}) %>
      <%= require("ejs-loader!./icon.ejs")({icon:"circle-icon-apps"}) %>
    </div>
  </div>
</div>